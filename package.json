{
  "name": "doppler-webapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@kunukn/react-collapse": "^2.2.9",
    "@semantic-release/exec": "^3.3.8",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.1",
    "@types/intl-tel-input": "^17.0.1",
    "@types/jest": "^26.0.23",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-intl": "^3.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/url-parse": "^1.4.3",
    "axios": "^0.21.1",
    "axios-retry": "^3.1.9",
    "c3": "^0.7.20",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "eclint": "^2.8.1",
    "env-cmd": "^10.1.0",
    "es6-object-assign": "^1.1.0",
    "formik": "^2.2.6",
    "full-icu": "^1.3.4",
    "g": "^2.0.1",
    "history": "^5.0.0",
    "intl-tel-input": "^17.0.12",
    "jwt-decode": "^3.1.2",
    "polyfill-array-includes": "^2.0.0",
    "prettier": "^2.2.1",
    "prettier-check": "^2.0.0",
    "promise-polyfill": "^8.2.0",
    "query-string": "^7.0.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-ga": "^3.3.0",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.15.8",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^16.14.0",
    "semantic-release": "^15.14.0",
    "styled-components": "^4.4.1",
    "stylelint": "^13.12.0",
    "typescript": "^3.9.9",
    "url-parse": "^1.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "verify": "yarn prettier-check && yarn eclint && yarn test:ci && yarn stylelint",
    "build:production": "env-cmd -f .env.production yarn build",
    "build:qa": "env-cmd -f .env.qa yarn build",
    "build:int": "env-cmd -f .env.int yarn build",
    "build:development": "env-cmd -f .env.development yarn build",
    "build:demo": "env-cmd -f .env.demo yarn build",
    "test:ci": "cross-env CI=true react-scripts test --env=jest-environment-jsdom-sixteen",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "eclint": "eclint check  src/**/*.js",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css}\"",
    "prettier-check": "prettier-check \"src/**/*.{js,jsx,ts,tsx,css}\"",
    "stylelint": "stylelint \"src/**/*.{js,css}\" --config \"src/.stylelintrc.json\""
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "release": {
    "branches": [
      "master",
      "main"
    ],
    "dryRun": false,
    "noCi": true,
    "debug": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ],
    "verifyConditions": [
      "@semantic-release/github"
    ],
    "analyzeCommits": [
      "@semantic-release/commit-analyzer"
    ],
    "generateNotes": [
      "@semantic-release/release-notes-generator"
    ],
    "publish": [
      {
        "path": "@semantic-release/github"
      }
    ],
    "success": [
      "@semantic-release/github"
    ],
    "fail": [
      "@semantic-release/github"
    ]
  },
  "resolutions": {
    "react-helmet/react-side-effect": ">=2.1.0",
    "comment": "Resolution to avoid test warning: [...] Please update the following components: SideEffect(NullComponent)"
  },
  "devDependencies": {
    "jest-environment-jsdom-sixteen": "^1.0.3"
  }
}
