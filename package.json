{
  "name": "doppler-webapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@semantic-release/exec": "^3.3.5",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.7",
    "@testing-library/react-hooks": "^1.1.0",
    "@types/intl-tel-input": "^14.0.1",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-google-recaptcha": "^1.0.0",
    "@types/react-intl": "^2.3.17",
    "@types/react-router-dom": "^4.3.2",
    "axios": "^0.19.0",
    "eclint": "^2.8.1",
    "env-cmd": "^9.0.3",
    "es6-object-assign": "^1.1.0",
    "formik": "^1.5.8",
    "g": "^2.0.1",
    "history": "^4.9.0",
    "intl-tel-input": "^16.0.2",
    "jwt-decode": "^2.2.0",
    "polyfill-array-includes": "^2.0.0",
    "prettier": "^1.16.4",
    "prettier-check": "^2.0.0",
    "promise-polyfill": "^8.1.3",
    "query-string": "^6.8.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-ga": "^2.6.0",
    "react-google-recaptcha": "^1.1.0",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.9.0",
    "react-remarkable": "^1.1.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.1",
    "react-test-renderer": "^16.8.6",
    "semantic-release": "^15.13.18",
    "styled-components": "^4.3.2",
    "typescript": "^3.5.3",
    "url-parse": "^1.4.7"
  },
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "verify": "yarn prettier-check && yarn eclint && yarn test:ci",
    "build:production": "env-cmd -f .env.production yarn build",
    "build:qa": "env-cmd -f .env.qa yarn build",
    "build:int": "env-cmd -f .env.int yarn build",
    "build:development": "env-cmd -f .env.development yarn build",
    "test:ci": "CI=true react-scripts test",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eclint": "eclint check  src/**/*.js",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier-check": "prettier-check \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "release": {
    "branch": "master",
    "dryRun": false,
    "noCi": true,
    "debug": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ],
    "verifyConditions": [
      "@semantic-release/github"
    ],
    "analyzeCommits": [
      "@semantic-release/commit-analyzer"
    ],
    "generateNotes": [
      "@semantic-release/release-notes-generator"
    ],
    "publish": [
      {
        "path": "@semantic-release/github"
      }
    ],
    "success": [
      "@semantic-release/github"
    ],
    "fail": [
      "@semantic-release/github"
    ]
  }
}
